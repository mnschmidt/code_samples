<table border="0" cellspacing="0" cellpadding="7">
  <tr>
    <th>R number</th>
    <th>Event ID</th>
		<th>Status</th>
  </tr>
<% for event in @events %>
		<tr>
			<td class="reported-value"><%= event.generation_r %></td>
			<td class="reported-value"><%= event.event_id %></td>
			<td class="reported-value"><%= event.event_status %></td>
		</tr>
	<% end %>
</table>
